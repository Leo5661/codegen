        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@codegen/eslint-config": "workspace:*",
        "@codegen/typescript-config": "workspace:*",
        "@types/figlet": "^1.5.8",
        "@types/fs-extra": "^11.0.4",
        "@types/node": "^20.11.24",
        "rimraf": "^6.0.1",
        "tsup": "^6.3.0",
        "typescript": "^5.6.2"
    }
}