{
  "extends": "@codegen/typescript-config/nextjs.json",
  "compilerOptions": {
    "baseUrl": ".",
      "paths": {
        "@/*": ["./*"]
      },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/page.mdx"
  ],
  "exclude": [
    "node_modules"
  ]
}
